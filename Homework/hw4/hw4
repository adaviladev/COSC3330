	4374	= 	54 * 81
	4374 	= 0001 0001 0001 0110 
			= 0000 0000 0011 0110 * 0000 0000 0101 0001

			= 	0000 0000 0011 0110 * 0000 0000 0101 0001
			=		0000 0000 0101 0001

				+	0000 1010 0010 0000
				+	0000 0101 0001 0000 
				+	0000 0001 0100 0100
				+	0000 0000 1010 0010
				=	0001 0001 0001 0110
